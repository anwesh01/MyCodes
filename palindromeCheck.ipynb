    "def palindromeCheck(name):\n",
    "    var = name[::-1]\n",
    "    if var == name:\n",
    "        return 1\n",
    "    else:\n",
    "        return 0\n",
    "\n",
    "a = palindromeCheck(\"malayalam\")\n",
    "if a == 1:\n",
    "    print(\"String is a palindrome\")\n",
    "else:\n",
    "    print(\"String is not palindrome\")\n",
    "\n",
    "        "
